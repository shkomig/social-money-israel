{
  "name": "kesef-hevrati",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env WATCHPACK_POLLING=true next dev",
    "dev:3001": "cross-env WATCHPACK_POLLING=true next dev -H 127.0.0.1 -p 3001",
    "dev:3010": "cross-env WATCHPACK_POLLING=true next dev -H 127.0.0.1 -p 3010",
    "dev:turbopack": "cross-env WATCHPACK_POLLING=true next dev --turbopack",
    "dev:turbopack:3001": "cross-env WATCHPACK_POLLING=true next dev -H 127.0.0.1 -p 3001 --turbopack",
    "dev:safe": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-reset.ps1 -Port 3001",
    "dev:safe:3010": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-reset.ps1 -Port 3010",
    "prestart:3001": "npx --yes kill-port 3001",
    "build": "next build",
    "start": "next start",
    "start:3001": "cross-env WATCHPACK_POLLING=true next start -H 127.0.0.1 -p 3001",
    "start:3010": "cross-env WATCHPACK_POLLING=true next start -H 127.0.0.1 -p 3010",
    "predev:3001": "npx --yes kill-port 3001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "generate:images": "node ./scripts/generate-logos.js"
  ,"convert:video": "node ./scripts/convert-video.js"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "jspdf": "^2.5.2",
    "next": "15.4.5",
    "nodemailer": "^6.10.1",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.18",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "^3.3.3",
    "sharp": "^0.32.6",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
  "typescript-eslint": "^8.13.0",
  "ffmpeg-static": "^5.1.0",
  "fluent-ffmpeg": "^2.1.2"
  }
}
